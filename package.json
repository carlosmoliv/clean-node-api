{
  "name": "clean-node-api",
  "version": "1.9.3",
  "description": "Nodejs Rest API using TDD, Clean Architecture and TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 dist/main/server.js",
    "up": "yarn build && docker compose up",
    "down": "docker compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test -- --watch -c jest.config.unit.ts",
    "test:integration": "yarn test -- --watch -c jest.config.integration.ts",
    "test:staged": "yarn test",
    "test:ci": "yarn test -- --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@shelf/jest-mongodb": "^4.1.7",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.15",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.9.4",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "mockdate": "^3.0.5",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mongodb": "^5.2.0",
    "nodemon": "^2.0.22",
    "swagger-ui-express": "^4.6.3",
    "validator": "^13.9.0"
  },
  "engines": {
    "node": ">=18.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
